{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Covid-reporting-Sri"
		},
		"ls_ablob_resportingsa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_resportingsa'"
		},
		"ls_adls_reportingdl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_reportingdl'"
		},
		"ls_adls_reportingdl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidreportingsridl.dfs.core.windows.net/"
		},
		"ls_http_opendata_ecdc_europa_data_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().sourceBaseURL}"
		},
		"tr_ingest_population_data_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/13f7a9a9-0be4-4f51-9fe5-5fb2df8d77c2/resourceGroups/Covid-reporting/providers/Microsoft.Storage/storageAccounts/covidreportingsri"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Covid-reporting-Sri_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_resportingsa_connectionString": {
						"value": "[parameters('ls_ablob_resportingsa_connectionString')]"
					},
					"ls_adls_reportingdl_accountKey": {
						"value": "[parameters('ls_adls_reportingdl_accountKey')]"
					},
					"ls_adls_reportingdl_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_reportingdl_properties_typeProperties_url')]"
					},
					"ls_http_opendata_ecdc_europa_data_properties_typeProperties_url": {
						"value": "[parameters('ls_http_opendata_ecdc_europa_data_properties_typeProperties_url')]"
					},
					"tr_ingest_population_data_properties_typeProperties_scope": {
						"value": "[parameters('tr_ingest_population_data_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}